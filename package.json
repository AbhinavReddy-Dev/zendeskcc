    "express-pino-logger": "^4.0.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^27.3.1",
    "msw": "^0.35.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.4",
